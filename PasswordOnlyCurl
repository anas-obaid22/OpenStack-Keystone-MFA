curl -i -s -X POST http://127.0.0.1/identity/v3/auth/tokens \
  -H "Content-Type: application/json" \
  -d '{
    "auth": {
      "identity": {
        "methods": ["password"],
        "password": {
          "user": {
            "name": "admin",
            "domain": {"name": "Default"},
            "password": "YOUR_ADMIN_PASSWORD"
          }
        }
      }
    }
  }'
